<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" media="screen" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <header>
            <div id="title">
                <h1>Agriculture Helper</h1>
            </div>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
            Created for <a href="http://www.rhok.org/event/waterhackathon-tel-aviv">WaterHackathon Tel Aviv 2011</a> by Michael Greenbaum, Tamar Barkai, Sharon Makmal & Amir Grozki
        </footer>
    </div>
</body>
</html>
